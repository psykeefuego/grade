#include<stdio.h>
 int main()
{   
  int p;
  printf("Enter marks");
  scanf("%d",&p);
  if(p>=85 && p<=100)
  {
  	printf("Grade A");
  }
  else if(p>=70 && p<=84)
  {
  	printf("Grade B");
  }
  else if(p>=55 && p<=69)
  {
  	printf("Grade C");
  }
  else if(p>=40 && p<=54)
  {
  	printf("Grade D");
  }
  else if(p<40)
  {
  	printf("Grade F");
  }
  else
  {
  	printf("Invalid Entry");
  }
}
