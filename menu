#include<stdio.h>
int main()
{   
    int n;
    printf("Enter a number");
    scanf("%d",&n);
    switch(n)
    {
    	case 1: printf("  Sandwich \n Rs.149 ");
    	        break;
    	case 2: printf("  Fries \n Rs.99 ");
    	        break;
    	case 3: printf("  Pasta \n Rs.179 ");
    	        break;
    	case 4: printf("  Pizza \n Rs.239 ");
    	        break;
    	case 5: printf("  Burger \n Rs.129 ");
    	        break;
    	case 6: printf("  Taco \n Rs.159");
    	        break;
    	default: printf(" Invalid entry");
	}
	
}
	
